{% for user in users %}
    <a href="{{ path('account.show',{'id':user.id}) }}">
        <div class="single-compte d-flex align-items-center mt-10 mb-10 mt-10Mobile mb-10Mobile">
            <div class="compte-picture">
                <img src="{{ vich_uploader_asset(user, 'imageFile') | imagine_filter('user_mini_thumbnail')  }}" alt="user">
            </div>
            <p class="mb-0">{{ user }}</p>
        </div>
    </a>
{% endfor %}

